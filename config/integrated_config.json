{
  "bigquery": {
    "project": "wprojectl",
    "dataset_lab": "lab",
    "dataset_draw": "draw",
    "location": "US",
    "timezone": "Asia/Shanghai"
  },
  "data_source": {
    "fallback_to_bigquery": true,
    "sync_to_bigquery": true,
    "validation_enabled": true
  },
  "monitoring": {
    "log_level": "INFO",
    "enable_metrics": true,
    "health_check_interval": 300,
    "data_freshness_threshold": 600
  },
  "backfill": {
    "batch_size": 100,
    "max_days_per_request": 7,
    "enable_deduplication": true,
    "validate_data_integrity": true
  }
}